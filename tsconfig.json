{
	"ts-node": {
		"transpileOnly": true,
		"require": ["tsconfig-paths/register"]
	},
	"compilerOptions": {
		"target": "ES3",
		"outDir": "",
		"outFile": "./build/datepicker.js",
		"baseUrl": "./",
		"paths": {
			"@/*": ["./src/*"]
		},
		"strict": true,
		"skipLibCheck": true,
